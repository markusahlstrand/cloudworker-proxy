    "test:integration": "node integration/run.js",
    "unit": "bun test"
  },
  "dependencies": {
    "aws4fetch": "1.0.13",
    "cloudworker-router": "1.11.2",
    "cookie": "0.4.1",
    "hono": "^3.6.0",
