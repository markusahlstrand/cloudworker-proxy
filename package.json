    "lodash.set": "4.3.2",
    "shortid": "2.2.16"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "bun": "1.0.0",
    "chai": "4.2.0",
    "dotenv": "8.2.0",
    "esbuild": "^0.19.2",
    "eslint": "7.13.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "fetch-mock": "9.11.0",
    "node-fetch": "2.6.1",
    "prettier": "2.1.2",
    "typescript": "^5.2.2",
    "wrangler": "^3.7.0"
  }
}
